<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GreenPlusERP.ViewModels"
                    xmlns:view="clr-namespace:GreenPlusERP.Views"
                    >
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ProductViewModel}">
        <view:ProductView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:CadastroUsuarioViewModel}">
        <view:CadastroUsuarioView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:CadastroFornecedorViewModel}">
        <view:CadastroFornecedorView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PlantioViewModel}">
        <view:PlantioView/>
    </DataTemplate>
</ResourceDictionary>